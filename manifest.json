{
  "manifest_version": 2,

  "name": "Youtube Feed Clean",
  "description": "This extension cleans stupid items from the Youtube feed",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["jquery.js", "script.js"]
    }
  ],
  "permissions": [
   "https://www.youtube.com/"
  ]
}